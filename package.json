{
  "name": "wakeflow-design",
  "version": "0.2.2",
  "description": "Wakeflow's component library",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "keywords": [
    "design",
    "wakeflow",
    "component library",
    "react"
  ],
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wakeflow/wakeflow-design.git"
  },
  "bugs": {
    "url": "https://github.com/wakeflow/wakeflow-design/issues"
  },
  "scripts": {
    "dev": "react-scripts start",
    "test": "mocha --parallel \"tests/**/*.test.js\" --timeout 5000",
    "lint": "eslint ./",
    "watch": "mocha --watch --parallel \"tests/**/*.test.js\" --timeout 5000",
    "watch-one": "mocha --watch --parallel \"tests/**/*.test.js\" --timeout 5000",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "chromatic": "npx chromatic --project-token=ef60da3b4f8c"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-react": "^7.16.7",
    "@iconify/react": "^3.1.1",
    "@storybook/addon-docs": "^6.4.19",
    "google-map-react": "^2.1.10",
    "jsonschema": "^1.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-hot-toast": "^2.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.17.2",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/node-logger": "^6.4.18",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.18",
    "babel-loader": "8.1.0",
    "chromatic": "^6.4.3",
    "eslint": "^7.32.0",
    "eslint-plugin-storybook": "^0.5.6",
    "mocha": "^9.1.3"
  }
}
