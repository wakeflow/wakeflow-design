{
  "name": "wakeflow-design",
  "version": "0.1.14",
  "description": "wakeflow's component library",
  "main": "dist/index.js",
  "type": "module",
  "keywords": [
    "design",
    "wakeflow",
    "component library",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wakeflow/wakeflow-design.git"
  },
  "bugs": {
    "url": "https://github.com/wakeflow/wakeflow-design/issues"
  },
  "scripts": {
    "dev": "react-scripts start",
    "test": "mocha --parallel \"tests/**/*.test.js\" --timeout 5000",
    "lint": "eslint ./",
    "watch": "mocha --watch --parallel \"tests/**/*.test.js\" --timeout 5000",
    "watch-one": "mocha --watch --parallel \"tests/**/*.test.js\" --timeout 5000",
    "build": "rm -rf dist && NODE_ENV=production babel src/components --out-dir dist --copy-files"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-react": "^7.16.0",
    "@iconify/react": "^3.1.1",
    "google-map-react": "^2.1.10",
    "jsonschema": "^1.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "eslint": "^7.32.0",
    "mocha": "^9.1.3"
  }
}
